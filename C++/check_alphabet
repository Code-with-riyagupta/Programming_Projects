#include<iostream>
using namespace std;
int main(){
    char ch;
    cout<<"Enter the character"<<endl;
    cin>>ch;
    int ascii=(int)ch;
    if(ascii>=97 && ascii<=122){
    cout<<"Character is a lowercase alphabet"<<endl;
    }
     else if(ascii>=65 && ascii<=90){
    cout<<"Character is a uppercase alphabet"<<endl;
    }
     else{
    cout<<"Character is not an alphabet";
    }
}
